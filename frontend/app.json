{
  "expo": {
    "name": "explore-penn",
    "slug": "explore-penn",
    "version": "1.0.0",
    "scheme": "myapp",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/landing.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kestelle.explorepenn",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your custom message explaining why the app needs location access while in use.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Your custom message explaining why the app needs location access even when the app is in the background.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kestelle.explorepenn",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e7cdd263-bc4c-41be-8270-25e4d9ed1088"
      },
      "firebaseApiKey": "AIzaSyDVcPQ18HbuIL6zFc7EMHVSWOwbHHU5U2E",
      "firebaseAuthDomain": "explore-penn-f67e6.firebaseapp.com",
      "firebaseProjectId": "explore-penn-f67e6",
      "firebaseStorageBucket": "explore-penn-f67e6.appspot.com",
