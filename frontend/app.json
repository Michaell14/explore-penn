      "iosClientId": "414478752713-ikd3dsmt2og30ev6jf95q3j9h0ohiusj.apps.googleusercontent.com",
      "androidClientId": "414478752713-survvesc5vb87jja1dvvtjbckbsj952d.apps.googleusercontent.com"
    },
    "owner": "spark-pins"
  }
}
